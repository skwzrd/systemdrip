{
    "services": [
        "nginx",
        "qbittorrent-nox",
        "docker",
        "neo",
        "systemdrip",
        "smbd",
        "sshd"
    ],
    "properties": [
        "StateChangeTimestamp",
        "ActiveState",
        "Result",
        "MemoryCurrent",
        "CPUUsageNSec",
        "TasksCurrent",
        "ActiveEnterTimestamp",
        "ActiveExitTimestamp"
    ],
